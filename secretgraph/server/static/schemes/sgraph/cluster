cluster:Cluster a rdfs:Class ;
  rdfs:isDefinedBy </static/schemes/sgraph/cluster#> ;
  rdfs:label "Cluster-Root" ;
  rdfs:comment "Root for a Cluster" ;
  rdfs:domain rdfs:Resource .

cluster:EncryptedBox a rdfs:Class ;
  rdfs:isDefinedBy </static/schemes/sgraph/cluster#> ;
  rdfs:domain cluster:Cluster .

cluster:EncryptedBox.secrets a rdf:Bag ;
  rdfs:isDefinedBy </static/schemes/sgraph/cluster#> ;
  rdfs:label "Encrypted secrets (nonce:<type:b64secret>)" ;
  rdfs:domain cluster:EncryptedBox ;
  rdfs:range xsd:string .

cluster:EncryptedBox.tasks a rdf:Seq ;
  rdfs:isDefinedBy </static/schemes/sgraph/cluster#> ;
  rdfs:domain cluster:EncryptedBox ;
  rdfs:range xsd:string .

cluster:Cluster.boxes a rdf:Property;
  rdfs:isDefinedBy </static/schemes/sgraph/cluster#> ;
  rdfs:comment "Encrypted boxes for a Cluster" ;
  rdfs:domain cluster:Cluster ;
