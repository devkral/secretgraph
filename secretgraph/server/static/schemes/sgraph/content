@prefix value: </static/schemes/sgraph/content#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix cc: <https://creativecommons.org/ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

</static/schemes/sgraph/content#> a owl:Ontology ;
  dc:title "Secretgraph Content Value" ;
  dc:description "This is the spkcgraph format used by spkcspider." ;
  cc:license <https://creativecommons.org/publicdomain/zero/1.0/> .


value:ContentValue a rdfs:Class ;
  rdfs:isDefinedBy </static/schemes/sgraph/content#> ;
  rdfs:domain rdfs:Resource .

value:Key a rdfs:Class ;
  rdfs:isDefinedBy </static/schemes/sgraph/content#> ;
  rdfs:domain value:ContentValue .

value:nonce a rdf:value ;
  rdfs:isDefinedBy </static/schemes/sgraph/content#> ;
  rdfs:domain value:ContentValue ;
  rdfs:range xsd:base64Binary .

value:hash a rdf:value ;
  rdfs:isDefinedBy </static/schemes/sgraph/content#> ;
  rdfs:domain value:Key ;
  rdfs:range xsd:base64Binary.

value:ekey a rdf:value ;
  rdfs:isDefinedBy </static/schemes/sgraph/content#> ;
  rdfs:domain value:Key ;
  rdfs:range xsd:base64Binary.

value:keys a rdf:Property ;
  rdfs:isDefinedBy </static/schemes/sgraph/content#> ;
  rdfs:domain value:ContentValue ;
  rdf:Bag value:Key ;

value:econtent a rdf:value ;
  rdfs:isDefinedBy </static/schemes/sgraph/content#> ;
  rdfs:domain value:ContentValue ;
  rdfs:range xsd:base64Binary.
