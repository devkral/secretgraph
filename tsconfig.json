{
  "compilerOptions": {
    "outDir": "./webpack_bundles/",
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "downlevelIteration": true,
    "jsx": "react",
    "lib": [
      "ESNext",
      "dom"
    ],
    "plugins": [
      {
        "name": "ts-graphql-plugin",
        "schema": "schema.json",
        "tag": "gql"
      }
    ]
  },
  "exclude": [
    "node_modules",
    "webpack.config.*.js",
    "**/*.spec.ts"
  ]
}
